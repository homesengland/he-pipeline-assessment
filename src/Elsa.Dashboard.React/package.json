{
  "name": "customstencil",
  "version": "0.0.14",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/custom/custom.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "dist/loader/"
  ],
  "scripts": {
    "dev": "vite",
    "push": "npm publish --access public",
    "push:preview": "npm publish --access public --tag preview",
    "build": "tsc -b && vite build",
    "start": "react build --dev --watch --serve",
    "test": "react test --spec --e2e",
    "test.watch": "react test --spec --e2e --watchAll",
    "generate": "react generate",
    "unit-test": "jest"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.0",
    "@mui/material": "^6.1.0",
    "async-mutex": "^0.4.0",
    "axios": "^0.26.1",
    "construe": "^0.4.1",
    "el-transition": "^0.0.7",
    "lodash": "^4.17.21",
    "react-dom": "^18.3.1",
    "sortablejs": "^1.15.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitejs/plugin-react": "^4.3.1",
    "axios": "^0.26.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "jest": "^29.0.0",
    "jest-cli": "^29.0.0",
    "jest-environment-jsdom": "^29.3.1",
    "jest-junit": "^15.0.0",
    "puppeteer": "^10.0.0",
    "react": "^18.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "workbox-build": "^4.3.1"
  },
  "license": "MIT",
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": ".",
    "outputName": "junit.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  }
}
