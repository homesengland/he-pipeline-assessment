{
  "name": "@elsa-workflows/elsa-workflows-studio",
  "version": "2.13.0",
  "description": "Elsa Workflow Studio is a collection of web components to manage Elsa workflows stored on an Elsa Workflows server.",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/elsa-workflows-studio/elsa-workflows-studio.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "custom:build": "stencil build --verbose --config ./customSrc/stencil.config.ts",
    "workflow:build:proj": "npm run workflow:pack:release",
    "workflow:pack:release": "cross-env webpack --mode=production --env configuration=Release",
    "workflow:pack:debug": "cross-env webpack --mode=development --env configuration=Debug",
    "workflow:watch": "cross-env webpack --watch --mode=development --env configuration=Debug",
    "workflow:push": "npm publish --access public",
    "workflow:push:preview": "npm publish --access public --tag preview",
    "workflow:build": "npm run workflow:build:tailwind:prod && stencil build --config stencil.config.dist.ts",
    "workflow:build:dev": "stencil build --dev --config stencil.config.dev.ts",
    "workflow:start": "stencil build --watch --serve --config stencil.config.prod.ts",
    "workflow:start:dev": "stencil build --dev --watch --serve --config stencil.config.dev.ts",
    "workflow:test": "stencil test --spec --e2e",
    "workflow:test.watch": "stencil test --spec --e2e --watchAll",
    "workflow:generate": "stencil generate",
    "workflow:build:tailwind:dev": "cross-env NODE_ENV=development tailwind build -i ./tailwind.css -o ./src/globals/tailwind.css",
    "workflow:build:tailwind:prod": "cross-env NODE_ENV=production tailwind build -i ./tailwind.css -o ./src/globals/tailwind.css"
  },
  "dependencies": {
    "@antv/x6": "^1.33.1",
    "@auth0/auth0-spa-js": "2.2.0",
    "@capacitor/browser": "7.0.1",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@microsoft/signalr": "^6.0.3",
    "@stencil-community/postcss": "^2.2.0",
    "@stencil/core": "^4.4.0",
    "@stencil/state-tunnel": "^1.0.1",
    "@stencil/store": "^1.5.0",
    "@stencil-community/router": "1.0.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/d3": "^7.1.0",
    "@types/dagre-d3": "^0.6.3",
    "@types/jquery": "^3.5.14",
    "@types/jquery-mousewheel": "^3.1.9",
    "@typescript-eslint/parser": "8.37.0",
    "ajv": "^8.10.0",
    "ansi-regex": "^6.0.1",
    "async-mutex": "^0.3.2",
    "autoprefixer": "10.4.2",
    "copy-webpack-plugin": "^10.2.4",
    "copyfiles": "^2.4.1",
    "cronstrue": "^1.125.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.4",
    "d3": "7.9.0",
    "dagre": "^0.8.5",
    "dagre-d3": "^0.6.4",
    "el-transition": "^0.0.7",
    "eslint": "9.30.",
    "i18next": "^21.6.14",
    "i18next-wc": "^1.1.0",
    "js-event-bus": "^1.1.1",
    "json-to-json-schema": "^0.0.4",
    "jsplumb": "^2.15.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "monaco-editor": "^0.33.0",
    "postcss": "^8.4.8",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^7.4.2",
    "reflect-metadata": "^0.1.13",
    "rollup-plugin-node-polyfills": " 0.2.1",
    "sortablejs": " 1.15.6",
    "stencil-click-outside": "^1.8.0",
    "tailwindcss": "^3.0.23",
    "typedi": "^0.10.0",
    "typescript": "5.8.3",
    "typescript-tslint-plugin": "1.0.1",
    "uuid": "^8.3.2",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.22"
  }
}
